{
    "manifest_version": 3,
    "name": "write-only-twitter",
    "version": "1.0",
    "description": "Redirects everything to x.com/compose/post",
    "permissions": [
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "https://x.com/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}
